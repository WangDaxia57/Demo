var body=$("body"),nextUser=$("#userhidden").attr("user");container.on("click",".bookCollect",function(){var o=$(this).attr("data-id"),e=$(this);$.get("/api/v1/favorite/"+o,function(o){"901"==o.status?$("body").append(TepLoging()):"902"==o.status?collectBookFalse():"200"==o.status&&(e.hasClass("collect_")||(e.html("已收藏"),e.hasClass("btn-grey-s")?e.addClass("btn-white-s").removeClass("bookCollect").removeClass("btn-grey-s"):e.removeClass("bookCollect")),collectBookOk())})}).on("click","#sell-mybook",function(){if(delDailog(),""!=nextUser){var o=$(this).data("href");window.location.href=o}else $("body").append(TepLoging())});